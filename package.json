{
  "name": "spoyousy",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prep": "npm i -g pnpm && pnpm i",
    "dev": "next dev -p 29336",
    "build": "next build",
    "start": "next start -p 29336",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "build:webos": "node scripts/build-webos.js",
    "package:webos": "node scripts/package-webos.js",
    "deploy:webos": "node scripts/deploy-webos.js",
    "dev:webos": "concurrently \"npm run dev\" \"npm run watch:webos\"",
    "watch:webos": "nodemon --watch webos --ext js,json,html --exec \"npm run package:webos\"",
    "setup:webos": "node scripts/setup-webos.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.1",
    "googleapis": "^148.0.0",
    "next": "14.0.4",
    "react": "^18",
    "react-dom": "^18",
    "react-redux": "^9.1.0",
    "react-toastify": "^10.0.4",
    "spotify-web-api-node": "^5.0.2",
    "youtube-player": "^5.6.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/spotify-web-api-node": "^5.0.11",
    "@types/youtube-player": "^5.5.11",
    "autoprefixer": "^10.0.1",
    "canvas": "^3.1.0",
    "concurrently": "^8.2.2",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "nodemon": "^3.0.3",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "terser": "^5.39.0",
    "typescript": "^5"
  }
}